{
  "version": 2,
  "builds": [
    {
      "src": "src/api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/src/api/auth.ts"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/src/api/users.ts"
    },
    {
      "src": "/api/agenda/(.*)",
      "dest": "/src/api/agenda.ts"
    },
    {
      "src": "/api/health/(.*)",
      "dest": "/src/api/health.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/api/index.ts"
    }
  ],
  "functions": {
    "src/api/**/*.ts": {
      "runtime": "nodejs18.x"
    }
  }
}
